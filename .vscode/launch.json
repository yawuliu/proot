      {
        "name": "Attach to gdbserver",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/src/proot",
        "miDebuggerServerAddress": "127.0.0.1:1234",
        "cwd": "${workspaceRoot}", 
        "linux": {
          "MIMode": "gdb"
